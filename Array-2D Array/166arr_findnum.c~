// Finding a given number in array


#include<stdio.h>
#define max 5
int main()
{

	int a[max];
	int i,sm=2;
	printf("Enter %d numbers: ",max);
	for(i=0;i<max;i++)
	{
		scanf("%d",&a[i]);
	}	
	for(i=0;i<max;i++)
	{
		if(a[i]==sm)
		{
			printf("find\n");
			break;
		}
	}
		if(i==max)
		{
			printf("Not find\n");
		}
return 1;
}
	
